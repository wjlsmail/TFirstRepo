cc=g++ 
prog=bin/SetTest
deps=$(shell find ./ -name "*.h") 
src=$(shell find ./ -name "*.cpp") 
obj=$(src:%.cpp=%.o) 

$(prog):$(obj)
	@echo "start link" $(prom) "......"
	@echo $(cc) $(obj) -o $(prog) 
	@$(cc) $(obj) -o $(prog) 

%.o:%.cpp $(deps)
	@echo "start compile" $(prom) "......"
	@echo $(cc) -c $< -o $@
	@$(cc) -c $< -o $@

.PHONY : clean
clean:
	@echo "start clean......"
	@echo rm -rf $(obj) $(prog)
	@rm -rf $(obj) $(prog)

